#pragma kernel CSMain

struct Particle
{
    float3 position;
};

RWStructuredBuffer<Particle> buffer;

[numthreads(4,1,1)]
void CSMain(int3 threadID : SV_GroupThreadID, int3 dispatchID : SV_DispatchThreadID)
{
    Particle particle;
    particle.position = float3(10, 10, 10);
    buffer[dispatchID.x] = particle;
}